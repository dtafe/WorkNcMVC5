@model WorkNCInfoService.Mvc5.Models.WorkModels.WorkNC_UserPermission

@using (Html.BeginForm("SetWebPermission", "UserPermission", FormMethod.Get))
{
    @Html.AntiForgeryToken()

    @Html.DropDownListFor(model => model.WebPermission, new SelectList(new List<Object>
                                                                                    {
                                                                                        new {value="", text="" },
                                                                                        new {value="Member", text="Member" },
                                                                                        new {value="Chief", text="Chief" }
                                                                                    }, "value", "text", 2),
                                                                                    htmlAttributes: new { @class = "form-control", id= "WebPermission" })
}
