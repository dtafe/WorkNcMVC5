@model MVCtest.Models.WorkModels.WorkNC_UserPermission

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div>
        <div class="form-group">
            @Html.DropDownListFor(model => model.WebPermission, new SelectList(new List<Object> {
                                                                                new {value="", text=""},
                                                                                new {value="Member", text="Member"},
                                                                                new { value="Chief",text="Chief"}
                                                                                }, "value","text",2),
                                                                new { htmlAttributes = new { @class = "form-control", id="permission" } })
            @Html.ValidationMessageFor(model => model.WebPermission, "", new { @class = "text-danger" })
        </div>
    </div>
}
